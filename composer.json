{
    "name": "roberts/meals",
    "description": "Laravel Package for meal prep service",
    "keywords": [
        "roberts",
        "meals"
    ],
    "homepage": "https://github.com/roberts/meals",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Roberts",
            "email": "packages@drewroberts.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "drewroberts/blog": "^4.2.2",
        "drewroberts/media": "^3.4.0",
        "staudenmeir/eloquent-has-many-deep": "^1.13.1",
        "tipoff/addresses": "^2.7.0",
        "tipoff/authorization": "^2.7.1",
        "tipoff/checkout": "^2.5.2",
        "tipoff/discounts": "^2.1.1",
        "tipoff/fees": "^2.2.0",
        "tipoff/invoices": "^2.0.2",
        "tipoff/locations": "^2.8.0",
        "tipoff/notes": "^2.0.1",
        "tipoff/payments": "^2.5.1",
        "tipoff/products": "^2.0.1",
        "tipoff/refunds": "^2.2.0",
        "tipoff/seo": "^2.4.0",
        "tipoff/support": "^1.8.8",
        "tipoff/taxes": "^2.1.0",
        "tipoff/vouchers": "^2.3.1"
    },
    "require-dev": {
        "tipoff/test-support": "^1.4.1"
    },
    "autoload": {
        "psr-4": {
            "Roberts\\Meals\\": "src",
            "Roberts\\Meals\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roberts\\Meals\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roberts\\Meals\\MealsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
